msgstr "Kriegs-Blutspinne"

#: ../base/ufos/team_aliens.ufo:395
msgid "Hovernet"
msgstr "Hovernet"

#: ../base/ufos/team_aliens.ufo:467
msgid "Combat Hovernet"
msgstr "Kriegs-Hovernet"

#: ../base/ufos/team_humans.ufo:54
msgid "Phalanx"
msgstr "Phalanx"

#: ../base/ufos/team_names.ufo:672
msgid "Ares W"
msgstr "Ares W"

